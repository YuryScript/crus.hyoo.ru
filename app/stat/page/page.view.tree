$hyoo_crus_app_stat_page $mol_page
	title \ğŸ“Š Stat
	body_content /
		<= Charts $mol_list rows /
			<= Cpu $mol_chart
				graphs /
					<= Cpu_user $mol_plot_line
						title \CPU User (%)
						series_y <= cpu_user /
					<= Cpu_system $mol_plot_line
						title \CPU System (%)
						series_y <= cpu_system /
					<= Cpu_ruler_sec $mol_plot_ruler_vert
					<= Cpu_mark $mol_plot_mark_cross
						labels <= times /
						graphs /
							<= Cpu_user
							<= Cpu_system
			<= Mem $mol_chart
				graphs /
					<= Mem_used $mol_plot_line
						title \Mem Alloc (%)
						series_y <= mem_used /
					- <= Fs_used $mol_plot_line
						title \FS Alloc (%)
						series_y <= fs_used /
					<= Mem_ruler $mol_plot_ruler_vert
					<= Mem_mark $mol_plot_mark_cross
						labels <= times /
						graphs /
							<= Mem_used
							- <= Fs_used
			<= Fs_acting $mol_chart
				graphs /
					<= Fs_read $mol_plot_line
						title \FS Read (pct/s)
						series_y <= fs_read /
					<= Fs_write $mol_plot_line
						title \FS Write (pct/s)
						series_y <= fs_write /
					<= Fs_ruler_pct $mol_plot_ruler_vert
					<= Fs_acting_mark $mol_plot_mark_cross
						labels <= times /
						graphs /
							<= Fs_read
							<= Fs_write
